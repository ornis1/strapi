{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blogs",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Blog title",
      "unique": true,
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "btnText": {
      "type": "string",
      "default": "Read more",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "meta.seo"
    }
  }
}
